<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mx.mc.mapper.TipoUsuarioMapper" >
 
    <select id="obtenerLista" resultType="mx.mc.model.TipoUsuario" parameterType="mx.mc.model.TipoUsuario" >
        SELECT idTipoUsuario, descripcion
        FROM tipoUsuario
        WHERE activo=1;
    </select>
</mapper>    