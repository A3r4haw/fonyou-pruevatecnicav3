<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mx.mc.mapper.AlteracionMapper" >
    
    <select id="obtenerListaAlteraciones" resultType="mx.mc.model.Alteracion" parameterType="Map">
        SELECT idAlteracion, nombreAlteracion, activa, insertFecha, insertIdUsuario 
            FROM alteracion;
    </select>
    
</mapper>  