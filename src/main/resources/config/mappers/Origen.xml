<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mx.mc.mapper.OrigenMapper" >
    <select id="obtenerLista" resultType="mx.mc.model.Origen" parameterType="Map" >
        SELECT * FROM origen
        WHERE 1=1
        <if test="prof == true" >
            AND idOrigen in (3,4)
        </if>
    </select>
</mapper>    