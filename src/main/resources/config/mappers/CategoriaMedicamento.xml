<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mx.mc.mapper.CategoriaMedicamentoMapper">
    
    <select id="obtenerTodo" resultType="mx.mc.model.CategoriaMedicamento" parameterType="mx.mc.model.CategoriaMedicamento">
        SELECT  idCategoriaMedicamento, nombreCategoriaMedicamento 
        FROM categoriaMedicamento
        WHERE 1=1
    </select>
    
</mapper>
