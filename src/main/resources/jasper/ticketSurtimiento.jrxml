	<subDataset name="reabasto1" whenResourceMissingType="Empty" uuid="a1084933-5681-4226-aad5-559f4fbe1b6b">
		<parameter name="folioReceta" class="java.lang.String"/>
		<parameter name="folioSurtimiento" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT me.claveInstitucional AS claveInterna, me.nombreCorto AS nombreComercial , pr.folio AS folioPrescripcion
, su.folio AS folioSurtimiento
, if((select valor from config where nombre = 'fun_surtimiento_receta_mixta' ) = 0 ,
	ifnull( round ( si.cantidadEnviada / i.cantidadXCaja , 0 ),0)
    , si.cantidadEnviada ) AS cantidadSurtida
FROM prescripcion pr
INNER JOIN prescripcionInsumo pi ON pr.idPrescripcion = pi.idPrescripcion
INNER JOIN surtimiento su ON pr.idPrescripcion = su.idPrescripcion
INNER JOIN surtimientoInsumo si ON ( su.idSurtimiento = si.idSurtimiento AND pi.idPrescripcionInsumo = si.idPrescripcionInsumo )
LEFT JOIN surtimientoEnviado se on si.idSurtimientoInsumo = se.idSurtimientoEnviado
LEFT JOIN inventario i ON se.idInventarioSurtido = i.idInventario
INNER JOIN medicamento me ON me.idMedicamento = pi.idInsumo

WHERE su.folio = $P{folioSurtimiento}
AND pr.folio = $P{folioReceta};]]>
		</queryString>
		<field name="claveInterna" class="java.lang.String"/>
		<field name="nombreComercial" class="java.lang.String"/>
		<field name="folioPrescripcion" class="java.lang.String"/>
		<field name="folioSurtimiento" class="java.lang.String"/>
		<field name="cantidadSurtida" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
