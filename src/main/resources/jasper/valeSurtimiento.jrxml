	<queryString>
		<![CDATA[select uuid() as id,m.claveInstitucional,m.nombreCorto,i.cantidadSolicitada,ifnull(i.cantidadEnviada,0) as cantidadEnviada,ifnull(i.cantidadVale,0) as cantidadVale,i.folioVale, 'ORIGINAL' as nota
from surtimientoInsumo i
inner join surtimiento s on s.idSurtimiento= i.idSurtimiento
inner join prescripcionInsumo pi on pi.idPrescripcionInsumo = i.idPrescripcionInsumo
inner join medicamento m on m.idMedicamento= pi.idInsumo
where   s.folio=$P{folioSurtimiento}
and i.cantidadVale>0
and i.folioVale is not null
Union all
select uuid() as id,m.claveInstitucional,m.nombreCorto,i.cantidadSolicitada,ifnull(i.cantidadEnviada,0) as cantidadEnviada,ifnull(i.cantidadVale,0) as cantidadVale,i.folioVale, 'C O P I A' as nota
from surtimientoInsumo i
inner join surtimiento s on s.idSurtimiento= i.idSurtimiento
inner join prescripcionInsumo pi on pi.idPrescripcionInsumo = i.idPrescripcionInsumo
inner join medicamento m on m.idMedicamento= pi.idInsumo
where   s.folio=$P{folioSurtimiento}
and i.cantidadVale>0
and i.folioVale is not null;]]>
	</queryString>
